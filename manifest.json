{
    "manifest_version": 3,
    "name": "Mini-Bloqueur",
    "version": "1.0",
    "description": "Bloque les sites distractifs",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage", "webNavigation", "tabs"]
  }